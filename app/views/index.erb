<%= erb :_navbar %>

<main class="container">

  <div class="form-group">
    <label for="user-search-input">Enter a card:</label>
    <input type="text" class="form-control" id="user-search-input">
  </div>

  <ul id="card-search-results" class="list-group">
  </ul>

  <div class="row">
    <div class="trade-window left col-md-6">
      <h2>Jaya Ballard <span id="jaya-total"></span></h2>
      <ul class="cardList" id="jaya-list" data-character="jaya">
      </ul>
    </div>
    <div class="trade-window right col-md-6">
      <h2>Squee Nabob <span id="squee-total"></span></h2>
      <ul class="cardList" id="squee-list" data-character="squee">
      </ul>
    </div>
  </div>

</main>

<%= erb :_hb_templates %>